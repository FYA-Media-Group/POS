function array_multi_search( $array, $index, $pattern, $invert = FALSE )
{
   $output = array();
   foreach ( $array as $i => $arr )
   {
	   // The index must exist and match the pattern
	   if ( isset( $arr[ $index ] ) && ( bool ) $invert !== ( bool ) preg_match( $pattern, $arr[ $index ] ) )
	   {
		   $output[ $i ] = $arr;
	   }
   }
	 return $output;
}



