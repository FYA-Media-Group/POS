function getMonthSpelling($Month)
{
	switch ($Month) {
    case "1":
        return "January";
        break;
    case "2":
        return "February";
        break;
    case "3":
        return "March";
        break;
    case "4":
        return "April";
        break;
    case "5":
        return "May";
        break;
    case "6":
        return "June";
        break;
    case "7":
        return "July";
        break;
    case "8":
        return "August";
        break;
    case "9":
        return "September";
        break;
    case "10":
        return "October";
        break;
    case "11":
        return "November";
        break;
    case "12":
        return "December";
        break;
    default:
        return "Invalid Month Number!";
}
}

function get12hour($Time24Hour)
{
	$DBSuitableAppointmentTime = $Time24Hour;
	$arr_Time = array();
	$arr_Time = explode(":","$DBSuitableAppointmentTime");
	if($arr_Time[0] < 12)
		$SuitableAppointmentTime = $arr_Time[0].":".$arr_Time[1]." AM";
	elseif($arr_Time[0] > 12)
		$SuitableAppointmentTime = ($arr_Time[0]-12).":".$arr_Time[1]." PM";
	elseif($arr_Time[0] == 12)
		$SuitableAppointmentTime = $arr_Time[0].":".$arr_Time[1]." PM";
	return $SuitableAppointmentTime;
}

